#!/bin/bash

####enableUpdateNotification

test -d ~/.config/autostart-disabled || mkdir -p ~/.config/autostart-disabled

if cp ~/.config/autostart/update-notifier.desktop ~/.config/autostart-disabled/update-notifier.desktop; then
    rm ~/.config/autostart/update-notifier.desktop & echo 'Enabled ok\n'
else
    echo 'Failed to enable or enabled already\n'
fi