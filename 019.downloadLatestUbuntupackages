#!/bin/bash

#### downloadLatestUbuntupackages

mkdir -p ~/.ubunsys/downloads
wget https://github.com/adgellida/ubuntupackages/archive/master.zip -O ~/.ubunsys/downloads/master.zip

unzip ~/.ubunsys/downloads/master.zip -d ~/.ubunsys/downloads
chmod 777 -R ~/.ubunsys/downloads/ubuntupackages-master
rm ~/.ubunsys/downloads/master.zip