#!/bin/bash

####disableUpdateNotification

test -d ~/.config/autostart-disabled || mkdir -p ~/.config/autostart-disabled

if ~/.config/autostart/update-notifier.desktop cp ~/.config/autostart-disabled/update-notifier.desktop; then
    rm ~/.config/autostart/update-notifier.desktop && echo 'Disabled ok'
else
    echo 'Failed to disable or disabled already'
fi